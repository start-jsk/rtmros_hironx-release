<package>

  <name>hironx_moveit_config</name>
  <version>2.1.0</version>
  <description>
     An automatically generated package with all the configuration and launch files for using the HiroNX with the MoveIt Motion Planning Framework
  </description>
  <author email="uesnaola@gmail.com">Urko Esnaola</author>
  <author email="iiysaito@opensource-robotics.tokyo.jp">Isaac I.Y. Saito</author>

  <maintainer email="k-okada@jsk.t.u-tokyo.ac.jp">Kei Okada</maintainer>
  <maintainer email="dev@opensource-robotics.tokyo.jp">TORK</maintainer>

  <license>BSD</license>

  <url type="website">http://wiki.ros.org/hironx_moveit_config</url>
  <url type="repository">https://github.com/start-jsk/rtmros_hironx</url>
  <url type="bugtracker">https://github.com/start-jsk/rtmros_hironx/issues</url>

  <buildtool_depend>catkin</buildtool_depend>
  
  <build_depend>collada_urdf</build_depend>
  <build_depend>hironx_ros_bridge</build_depend>
  <build_depend>moveit_ros_move_group</build_depend>

  <run_depend>hironx_ros_bridge</run_depend>
  <run_depend>moveit_planners</run_depend>
  <run_depend>moveit_ros</run_depend>
  <run_depend>moveit_simple_controller_manager</run_depend>
  
  <test_depend>rostest</test_depend>
</package>
